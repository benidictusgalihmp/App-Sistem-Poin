<?xml version="1.0" encoding="UTF-8"?>
<odoo>

  <record model="ir.ui.view" id="anggota_anggota_tree">
    <field name="name">anggota.anggota.tree</field>
    <field name="model">anggota.anggota</field>
    <field name="priority" eval="1"/>
    <field name="arch" type="xml">
      <tree>
        <field name="id_anggota"/>
        <field name="name"/>
        <field name="department"/>
        <field name="poin"/>
      </tree>
    </field>
  </record>

  <record model="ir.ui.view" id="riwayat_riwayat_tree">
    <field name="name">riwayat.riwayat.tree</field>
    <field name="model">riwayat.riwayat</field>
    <field name="priority" eval="1"/>
    <field name="arch" type="xml">
      <tree create="false">
        <field name="id_anggota"/>
        <field name="nama"/>
        <field name="poin_lama"/>
        <field name="transaksi"/>
        <field name="poin_baru"/>
        <field name="write_date"/>
      </tree>
    </field>
  </record>
  
</odoo>